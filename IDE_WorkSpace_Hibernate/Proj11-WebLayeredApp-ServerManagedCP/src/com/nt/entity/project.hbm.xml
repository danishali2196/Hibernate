<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        <hibernate-mapping>
        	<class name="com.nt.entity.Project"  table="PROJECT">
        		<id name="projId" column="PROJID" length="5" type="int"/>
        		<property name="projName" column="PROJNAME" length="20" type="string"/>
        		<property name="teamSize" column="TEAMSIZE" length="2" type="int"/>
        		<property name="domain"  column="DOMAIN" length="10" type="string" />
        		<property name="sdp" column="SDP" type="date"/>
        		<property name="edcp" column="EDCP" type="date"/>
        		
        	</class>
        </hibernate-mapping>